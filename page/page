function resizeDiv(islasttry){
	var WX,WY,BX,BY;
	var o=document.getElementById("dVadbT7i"),t,d;
	if (!o) return;
	if(typeof window.self_getsizes == 'function'){
		var s=self_getsizes();
		if(s.err==1 && !islasttry) return;
		if(isNaN(s.BX)) s.BX==0;
		if(isNaN(s.BY)) s.BY==0;
		if(s.err==1){
			if (!(t=document.getElementById("bannerXadbT7i"))) return;
			else s.BX=t.value;
			if (!(t=document.getElementById("bannerYadbT7i"))) return;
			else s.BY=t.value;
		}
		BX=s.BX;
		BY=s.BY;
	}else{
